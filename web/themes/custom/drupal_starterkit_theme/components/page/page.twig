{#
/**
 * @file
 * Template for the main page.
 */
#}
{% set page_attributes = page_attributes ?: create_attribute() %}
{% set
  page_classes = [
    'page',
    'node-type--' ~ node.bundle,
  ]|merge(page_utility_classes ? page_utility_classes : [])
%}
<div {{ page_attributes.addClass(page_classes) }}>
  {% block page_navigation %}
    {% include 'drupal_starterkit_theme:page-navigation' %}
  {% endblock %}

  {% block page_content %}
    {% include 'drupal_starterkit_theme:page-content' %}
  {% endblock %}

  {% block page_footer %}
    {% include 'drupal_starterkit_theme:page-footer' %}
  {% endblock %}
</div>
